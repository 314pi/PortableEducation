<?xml version="1.0" ?>
<resource>
  
  <object class="wxPanel" name="SourcesPanel">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <object class="sizeritem">
            <object class="wxPanel">
              <object class="wxBoxSizer">
                <object class="sizeritem">
                  <object class="wxListBox" name="sources_list"/>
                  <option>1</option>
                  <flag>wxALL|wxGROW</flag>
                  <border>0</border>
                </object>
                <orient>wxVERTICAL</orient>
                <object class="sizeritem">
                  <object class="wxPanel" name="toolbar_panel"/>
                  <option>0</option>
                  <flag>wxALL|wxGROW</flag>
                  <border>6</border>
                </object>
              </object>
            </object>
            <option>2</option>
            <flag>wxALL|wxGROW</flag>
            <border>6</border>
          </object>
          <object class="sizeritem">
            <object class="wxPanel" name="properties_panel">
              <object class="wxFlexGridSizer">
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>Name</label>
                  </object>
                  <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  <border>6</border>
                </object>
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="source_name"/>
                  <flag>wxGROW</flag>
                </object>
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>URL</label>
                  </object>
                  <flag>wxALIGN_CENTRE_VERTICAL</flag>
                </object>
                <object class="sizeritem">
                  <object class="wxBoxSizer">
                    <object class="sizeritem">
                      <object class="wxTextCtrl" name="source_location"/>
                      <option>1</option>
                      <flag>wxRIGHT|wxGROW</flag>
                      <border>6</border>
                    </object>
                    <object class="sizeritem">
                      <object class="wxButton" name="directory_picker">
                        <label>...</label>
                        <style>wxBU_BOTTOM|wxBU_EXACTFIT</style>
                      </object>
                    </object>
                    <orient>wxHORIZONTAL</orient>
                  </object>
                  <option>1</option>
                  <flag>wxGROW</flag>
                </object>
                <cols>2</cols>
                <rows>2</rows>
                <vgap>6</vgap>
                <hgap>6</hgap>
                <growablecols>1</growablecols>
                <object class="sizeritem">
                  <object class="wxStaticText">
                    <label>Repository Directory</label>
                  </object>
                  <flag>wxALIGN_CENTRE_VERTICAL</flag>
                </object>
                <object class="sizeritem">
                  <object class="wxTextCtrl" name="directory"/>
                  <flag>wxGROW|wxALIGN_CENTRE_VERTICAL</flag>
                </object>
              </object>
            </object>
            <option>3</option>
            <flag>wxALL|wxGROW</flag>
            <border>6</border>
          </object>
          <orient>wxHORIZONTAL</orient>
        </object>
        <option>1</option>
        <flag>wxGROW</flag>
        <border>0</border>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <object class="sizeritem">
            <object class="wxCheckBox" name="gui_passive_ftp">
              <label>Passive FTP Transfer</label>
            </object>
            <option>1</option>
            <flag>wxALL|wxGROW</flag>
          </object>
          <object class="sizeritem">
            <object class="wxStdDialogButtonSizer">
              <object class="button">
                <object class="wxButton" name="wxID_OK"/>
              </object>
              <object class="button">
                <object class="wxButton" name="wxID_CANCEL"/>
              </object>
            </object>
            <option>1</option>
            <flag>wxALL|wxGROW</flag>
          </object>
          <orient>wxHORIZONTAL</orient>
        </object>
        <option>0</option>
        <flag>wxALL|wxGROW</flag>
        <border>6</border>
      </object>
    </object>
  </object>
</resource>