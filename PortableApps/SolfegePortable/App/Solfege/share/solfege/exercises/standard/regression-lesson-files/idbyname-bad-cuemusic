header {
    lesson_id = "a4ed9d94-d3c0-4dd9-9dfb-b8c784f99e7a"
    module = idbyname
    title = "1 of 2 questions contain bad mpd code cuemusic"
}

question {
 name = "A"
 set=0
 music = rvoice("c d e")
 cuemusic = rvoice("c d e")
}

question {
 name= "B"
 set=0
 music = rvoice("c e c c c")
 cuemusic = rvoice("c e c ERR c c")
}

