header {
  module = rhythmdictation2
  title = _("3/4 - %s") % 1
}

countin = rhythm("\time 3/4 d4 d d")
tempo = 120/4

question {
  bars = 3/4, 3/4, 3/4, 3/4
  elements = "4", "2"
}

