header {
  module = rhythmdictation2
  title = _("3/4 - %s") % 3
}

tempo = 120/4
countin = rhythm("\time 3/4 d4 d d")

question {
  bars = 3/4, 3/4
  elements = "8 4 8", "8 8", "4", "2"
}

