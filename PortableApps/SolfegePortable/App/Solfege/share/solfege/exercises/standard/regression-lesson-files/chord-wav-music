header {
    lesson_id = "ffc11578-9893-4a15-abda-57a04c55a9bf"
    module = chord
        random_transpose = accidentals, -5, 5
        title = "1 of 2 questions have a missing wav file"
}
question { name=_("major") set=0 inversion=0 wavfile("share/sound.wav") }
question { name=_("major") set=1 inversion=1 wavfile("share/x-missing-file.wav")}

