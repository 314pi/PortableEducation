question {
 music = music("\staff\relative c'{c4 d8 e f4}\addvoice\relative c'{\stemDown c4 b8 c a4}")
 rhythm = rvoice("c4 cis8 d es4")
}

question {
 music = rhythm("c4 c8 c8")
 tempo = 100/4
}

